  "engines": {
    "node": "8.11.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "verbose": true,
    "bail": true,
    "moduleFileExtensions": [
      "vue",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "\\.(svg|woff2|css|scss)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "transform": {
      ".+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/docs/",
      "<rootDir>/static/",
      "<rootDir>/test/",
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ],
        "plugins": [
          "require-context-hook"
        ]
      }
    }
  }

{
    "name": "web",
    "description": "Web server",
    "repository": {},
    "license": "SEE LICENSE IN LICENSE.md",
    "version": "0.1.0",
    "scripts": {
        "watch": "tsc --listEmittedFiles -w",
        "build": "tsc --listEmittedFiles",
        "format": "./node_modules/.bin/tsfmt -r",
        "start": "npm run format && npm run build && ./node_modules/.bin/nodemon --exec node ./build/index.js",
        "test-api-call": "curl http://localhost:3000/api/v1/ping"
    },
    "semistandard": {
        "globals": [
            "after",
            "afterEach",
            "before",
            "beforeEach",
            "describe",
            "it"
        ]
    },
    "dependencies": {
        "express": "^4.16.4",
        "knex": "^0.15.2",
        "mysql": "^2.16.0",
        "objection": "^1.4.0",
        "swagger-express-validator": "^0.1.1",
        "swagger-parser": "^6.0.2",
        "swagger-routes-express": "^2.0.0"
    },
    "devDependencies": {
        "nodemon": "^1.18.6",
        "typescript": "^3.1.6",
        "typescript-formatter": "^7.2.2"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "env": {
            "development": {
                "presets": []
            },
            "production": {
                "presets": []
            },
            "test": {
                "presets": []
            }
        }
    }
}

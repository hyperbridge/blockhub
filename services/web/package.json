{
    "name": "web",
    "description": "Web server",
    "repository": {},
    "license": "SEE LICENSE IN LICENSE.md",
    "version": "0.1.0",
    "scripts": {
        "tsc": "tsc",
        "watch": "tsc -w",
        "build-development": "npm run format && tsc --listEmittedFiles",
        "build-production": "tsc",
        "format": "tsfmt -r",
        "start": "npm run build-production && node ./build/src/index.js",
        "dev": "npm run build-development && nodemon --exec node ./build/src/index.js",
        "migrate": "./node_modules/.bin/knex migrate:latest",
        "migrate-heroku": "heroku run 'knex migrate:latest'",
        "test-api-call": "curl http://localhost:3000/api/v1/ping"
    },
    "semistandard": {
        "globals": [
            "after",
            "afterEach",
            "before",
            "beforeEach",
            "describe",
            "it"
        ]
    },
    "dependencies": {
        "@feathersjs/authentication": "^2.1.13",
        "@feathersjs/authentication-jwt": "^2.0.7",
        "@feathersjs/authentication-local": "^1.2.7",
        "@feathersjs/express": "^1.2.7",
        "@feathersjs/feathers": "^3.2.3",
        "@feathersjs/socketio": "^3.2.7",
        "body-parser": "^1.18.3",
        "connect-session-knex": "^1.4.0",
        "dotenv": "^6.1.0",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "feathers-authentication": "^1.3.1",
        "feathers-hooks-common": "^4.19.0",
        "feathers-memory": "^2.2.0",
        "feathers-objection": "^1.2.4",
        "helmet": "^3.15.0",
        "knex": "^0.15.2",
        "knex-migrate": "^1.7.0",
        "morgan": "^1.9.1",
        "mysql": "^2.16.0",
        "objection": "^1.4.0",
        "pg": "^7.6.1",
        "swagger-express-validator": "^0.1.1",
        "swagger-parser": "^6.0.2",
        "swagger-routes-express": "^2.0.0",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "nodemon": "^1.18.6",
        "typescript": "^3.1.6",
        "typescript-formatter": "^7.2.2"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "env": {
            "development": {
                "presets": []
            },
            "production": {
                "presets": []
            },
            "test": {
                "presets": []
            }
        }
    }
}

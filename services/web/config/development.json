{
    "environment": "development",
    "facebook": {
        "authUri": "https://www.facebook.com/v2.9/dialog/oauth?client_id={app-id}&response_type=code&redirect_uri={redirect-uri}",
        "tokenUri": "https://graph.facebook.com/v2.9/oauth/access_token?client_id={app-id}&client_secret={client-secret}&code={code}&redirect_uri={redirect-uri}",
        "profileUri": "https://graph.facebook.com/me?fields=id,name,email,firstName,lastName&access_token={access_token}",
        "appId": "",
        "secret": "",
        "redirectUrl": "http://localhost:4000/fbAuthConfirm"
    },
    "google": {
        "authUri": "https://accounts.google.com/o/oauth2/v2/auth",
        "tokenUri": "",
        "profileUri": "",
        "clientId": "",
        "secret": "",
        "redirectUrl": "http://localhost:4000/googleAuthConfirm"
    },
    "grpc": {
        "host": "localhost:50051"
    },
    "service": {
        "port": 4000
    }
}